{
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "es6": true,
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-console": 0,
    "consistent-return": 0,
    "react/no-unescaped-entities": [false, {}],
    "react/prop-types": 0
  }
}